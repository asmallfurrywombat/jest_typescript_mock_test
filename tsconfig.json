{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "reactNamespace": "app",
    "lib": ["dom", "es2015", "es5"],
    "experimentalDecorators": true,
    "sourceMap": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "declaration": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "pretty": true,
    "outDir": "dist",
  },
  "formatCodeOptions": {
    "indentSize": 2,
    "tabSize": 2
  },
  "exclude": [
    "node_modules"
  ]
}